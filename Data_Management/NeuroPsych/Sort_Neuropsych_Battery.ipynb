   "source": [
    "#!/bin/bash\n",
    "\n",
    "for dir in */; do\n",
    "    if [[ -d \"$dir\" && \"$dir\" == *\"_\"* ]]; then\n",
    "        id=$(echo \"$dir\" | grep -oP '^[^_]+')\n",
    "        if [[ ! -d \"$id\" ]]; then\n",
    "            mkdir -p \"$id\"\n",
    "        fi\n",
    "        mv \"$dir\" \"$id/\"\n",
    "        echo \"$dir -> $id\"\n",
    "    fi\n",
    "done\n"
   ]
  }
